  <process id="myProcess" name="实名认证审批流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <serviceTask id="mailtask2" name="邮箱验证码" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${mailto}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[admin@atguigu.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[邮件验证码]]></activiti:string>
        </activiti:field>
        <activiti:field name="charset">
          <activiti:string><![CDATA[UTF-8]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>${emailcontent}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" sourceRef="startevent1" targetRef="mailtask2"></sequenceFlow>
    <userTask id="usertask1" name="邮件验证码确认" activiti:assignee="${memberid}"></userTask>
